	<portlet>
		<portlet-name>Simple IFrame Portlet</portlet-name>
		<display-name xml:lang="en">Simple IFrame Portlet</display-name>
		<display-name>Simple IFrame Portlet</display-name>
		<portlet-class>com.asponte.portal.designer.portlets.SimpleIFramePortlet</portlet-class>
		<init-param>
			<name>wps.markup</name>
			<value>html</value>
