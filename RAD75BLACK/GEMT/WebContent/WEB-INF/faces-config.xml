	<navigation-rule>
	<from-view-id>/GEMTNotes/GEMTNotesView.jsp</from-view-id>
	<navigation-case>
			<from-action>#{pc_GEMTNotesView.doLinkViewNotesAction}</from-action>
			<from-outcome>VIEW_PAGE</from-outcome>
			<to-view-id>/GEMTNotes/GEMTNotesFormView.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
<navigation-rule>
	<navigation-case>
			<from-action>#{pc_GEMTNotesFormView.doSubmitAction}</from-action>
			<from-outcome>VIEW_PAGE</from-outcome>
			<to-view-id>/GEMTNotes/GEMTNotesFormView.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
	<navigation-case>
			<from-action>#{pc_GEMTNotesFormView.doEditReportAction}</from-action>
			<from-outcome>VIEW_PAGE</from-outcome>
			<to-view-id>/GEMTNotes/GEMTNotesEdit.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
	<navigation-case>
			<from-action>#{pc_GEMTNotesFormView.doDeleteReportAction}</from-action>
			<from-outcome>VIEW_PAGE</from-outcome>
			<to-view-id>/GEMTNotes/GEMTNotesView.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/GEMTEntryForm/GEMTEntryFormView.jsp</from-view-id>
		<navigation-case>
			<from-action>#{pc_GEMTEntryFormView.doEditReportAction}</from-action>
			<to-view-id>/GEMTEntryForm/GEMTEntryFormReportEdit.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{pc_GEMTEntryFormView.doViewreportAction}</from-action>
			<to-view-id>/GEMTEntryForm/GEMTEntryFormReportView.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{pc_GEMTEntryFormView.doButton1Action}</from-action>
			<to-view-id>/GEMTEntryForm/GEMTEntryFormReportEdit.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{pc_GEMTEntryFormView.doLinkViewReportAction}</from-action>
			<to-view-id>/GEMTEntryForm/GEMTEntryFormReportView.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{pc_GEMTEntryFormView.doViewReportsAction}</from-action>
			<to-view-id>/GEMTEntryForm/GEMTEntryFormReportView.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>NEW_REPORT</from-outcome>
			<to-view-id>/GEMTEntryForm/GEMTEntryFormReportEdit.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>VIEW_REPORT</from-outcome>
			<to-view-id>/GEMTEntryForm/GEMTEntryFormReportView.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/GEMTEntryForm/GEMTEntryFormReportEdit.jsp</from-view-id>
		<navigation-case>
			<from-action>#{pc_GEMTEntryFormReportEdit.doCancelAction}</from-action>
			<from-outcome>CANCEL</from-outcome>
			<to-view-id>/GEMTEntryForm/GEMTEntryFormView.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{pc_GEMTEntryFormReportEdit.doRouteDocumentAction}</from-action>
			<from-outcome>ROUTE</from-outcome>
			<to-view-id>/GEMTEntryForm/GEMTEntryFormReportView.jsp</to-view-id>
		</navigation-case>		
		<navigation-case>
			<from-action>#{pc_GEMTEntryFormReportEdit.doToggleAction}</from-action>
			<to-view-id>/GEMTEntryForm/GEMTEntryFormReportView.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/GEMTEntryForm/GEMTEntryFormReportView.jsp</from-view-id>
		<navigation-case>
			<from-action>#{pc_GEMTEntryFormReportView.doCancelAction}</from-action>
			<to-view-id>/GEMTEntryForm/GEMTEntryFormView.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{pc_GEMTEntryFormReportView.doEditReportAction}</from-action>
			<from-outcome>EDIT_REPORT</from-outcome>
			<to-view-id>/GEMTEntryForm/GEMTEntryFormReportEdit.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{pc_GEMTEntryFormReportView.doShowParentReportAction}</from-action>
			<from-outcome>SHOW_PARENT_REPORT</from-outcome>
			<to-view-id>/GEMTEntryForm/GEMTEntryFormParentReport.jsp</to-view-id>
		</navigation-case>		
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/GEMTManager/GEMTManagerView.jsp</from-view-id>
		<navigation-case>
			<from-action>#{pc_GEMTManagerView.doAddUsersAction}</from-action>
			<from-outcome>ADD_USER</from-outcome>
			<to-view-id>/GEMTManager/GEMTManagerAddUsers.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/GEMTManager/GEMTManagerAddUsers.jsp</from-view-id>
		<navigation-case>
			<to-view-id>/GEMTManager/GEMTManagerView.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/GEMTWelcome/GEMTWelcomeView.jsp</from-view-id>
		<navigation-case>
			<from-action>#{pc_GEMTWelcomeView.doChange_passwordAction}</from-action>
			<from-outcome>THANKS</from-outcome>
			<to-view-id>/GEMTWelcome/GEMTWelcomeThanks.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/GEMTEntryForm/GEMTEntryFormParentReport.jsp</from-view-id>
		<navigation-case>
			<from-action>#{pc_GEMTEntryFormParentReport.doCancelAction}</from-action>
			<from-outcome>CANCEL</from-outcome>
			<to-view-id>/GEMTEntryForm/GEMTEntryFormReportView.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<lifecycle>
