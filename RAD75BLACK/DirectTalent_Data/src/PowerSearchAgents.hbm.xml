<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

<class name="com.manpower.portal.mpnet.hbn.beans.PowerSearchAgent" table="POWERSEARCHAGENTS">
		<id name="id" type="long" column="ID" >
			<generator class="sequence"> 
				<param name="sequence">POWERSEARCHAGENTS_SEQ</param>
			</generator>
		</id>

		<property name="siteId" type="long">
			<column name="SITE_ID" />
		</property>	
		<property name="recruiterId" type="long">
			<column name="RECRUITER_ID" />
		</property>	
		<property name="language" type="string">
			<column name="LANG" />
		</property>	
		<property name="searchType" type="string">
			<column name="SEARCHTYPE" />
		</property>
		<property name="keyword" type="string">
			<column name="KEYWORD" />
		</property>
		<property name="locationId">
			<column name="LOCATION_ID"/>
		</property>
		<property name="distanceUnit">
			<column name="DISTANCEUNIT" />
		</property>
		<property name="proximity">
			<column name="PROXIMITY" />
		</property>
		<property name="email">
			<column name="EMAIL" />
		</property>
		<property name="userId">
			<column name="USER_ID" />
		</property>
		<property name="firstName">
			<column name="FIRSTNAME" />
		</property>
		<property name="middleName">
			<column name="MIDDLENAME" />
		</property>
		<property name="lastName">
			<column name="LASTNAME"/>
		</property>
		<property name="phoneNumber">
			<column name="PHONENUMBER" />
		</property>
		<property name="branchId" type="long">
			<column name="BRANCH_ID" />
		</property>
		<property name="residenceStatus" type="string">
			<column name="RESIDENCESTATUS" />
		</property>
		<property name="educationLevel" type="string">
			<column name="EDUCATIONLEVEL" />
		</property>
		<property name="industry" type="string">
			<column name="INDUSTRY" />
		</property>
		<property name="workPermit" type="string">
			<column name="WORKPERMIT" />
		</property>
		<property name="agentName" type="string">
			<column name="AGENTNAME" />
		</property>
		<property name="active" type="boolean">
			<column name="ACTIVE" />
		</property>
		
 	</class>
</hibernate-mapping>
