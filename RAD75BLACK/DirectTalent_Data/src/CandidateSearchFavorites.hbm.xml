<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.manpower.portal.mpnet.hbn.beans.CandidateSearchFavorite" table="CANDIDATESEARCHFAVORITES">
		
		<id name="id" type="long" column="ID" >
			<generator class="sequence"> 
				<param name="sequence">CANDIDATESEARCHFAVORITES_SEQ</param>
			</generator>
		</id>

		<property name="candidateId" type="long">
			<column name="CANDIDATE_ID" />
		</property>	
		<property name="recruiterId" type="long">
			<column name="RECRUITER_ID" />
		</property>	
		<property name="comment">
			<column name="RECRUITER_COMMENT" />
		</property>
		
 	</class>
</hibernate-mapping>
