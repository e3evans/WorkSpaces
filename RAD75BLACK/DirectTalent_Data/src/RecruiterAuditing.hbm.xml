<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.manpower.portal.mpnet.hbn.beans.RecruiterAuditing" table="RECRUITER_AUDITING">
		<id name="id" type="long" column="ID" >
		<generator class="sequence"> 
		<param name="sequence">RECRUITER_AUDITING_SEQ</param>
		</generator>
		</id>
	
		<property name="site_id" type="long">
			<column name="SITE_ID" />
		</property>	
		<property name="recruiter_id" type="long">
			<column name="RECRUITER_ID" />
		</property>
		<property name="action">
			<column name="ACTION" />
		</property>
		<property name="created_On" type="timestamp">
			<column name="CREATED_ON" />
		</property>
		<property name="searchString">
			<column name="SEARCH_DATA" />
		</property>
		<property name="portletName">
			<column name="PORTLET" />
		</property>
		<property name="entity">
			<column name="ENTITY" />
		</property>	
		<property name="entityId">
			<column name="ENTITY_ID" />
		</property>	
		<property name="foundResult">
			<column name="FOUND_RESULT" />
		</property>										
 	</class>
</hibernate-mapping>
