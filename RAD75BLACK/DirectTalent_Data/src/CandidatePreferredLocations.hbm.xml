<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

<class name="com.manpower.portal.mpnet.hbn.beans.CandidatePreferredLocations" table="CANDIDATEPREFERREDLOCATIONS">
		<id name="id" type="long" column="ID" >
			<generator class="sequence"> 
				<param name="sequence">CANDIDATELOCATIONS_SEQ</param>
			</generator>
		</id>

		<property name="candidateId" column="CANDIDATE_ID" type="long"></property>	
		<property name="locationId" column="LOCATION_ID" type="long"></property>
		<property name="locationFlag" column="LOCATION_FLAG"></property>	
		
 	</class>
</hibernate-mapping>