<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

<class name="com.manpower.portal.mpnet.hbn.beans.CareerHarmony" table="CAREERHARMONY">
		<id name="id" type="long" column="ID" >
		<generator class="sequence"> 
		<param name="sequence">CAREERHARMONY_SEQ</param>
		</generator>
		</id>
		
		<!-- A cat has to have a name, but it shouldn' be too long. -->
		<property name="siteId" type="long">
			<column name="SITE_ID" />
		</property>
		<property name="securityKey">
			<column name="SECURITYKEY" />
		</property>
		<property name="projectID">
			<column name="PROJECT_ID" />
		</property>
		<property name="companyID">
			<column name="COMPANY_ID" />
		</property>
		<property name="languageId">
			<column name="LANGUAGE_ID" />
		</property>
		<property name="notification" type="long">
			<column name="NOTIFICATION" />
		</property>
		<property name="updatedOn" type="timestamp">
			<column name="UPDATED_ON" />
		</property>
		<property name="updatedBy">
			<column name="UPDATED_BY" />
		</property>
		<property name="created_On" type="timestamp">
			<column name="CREATED_ON" />
		</property>
		<property name="changed_On" type="timestamp">
			<column name="CHANGED_ON" />
		</property>
 	</class>
</hibernate-mapping>
