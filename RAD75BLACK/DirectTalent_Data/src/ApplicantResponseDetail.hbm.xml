<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

<class name="com.manpower.portal.mpnet.hbn.beans.ApplicantResponseDetail" table="APPLICANTRESPONSEDETAIL">
		<id name="id" type="long" column="ID" >
		<generator class="sequence"> 
			<param name="sequence">APPLICANTRESPONSEDETAIL_SEQ</param>
		</generator>
		</id>
		<!-- A cat has to have a name, but it shouldn' be too long. -->
		<property name="applicantResponseId" type="long">
			<column name="APPLICANTRESPONSEID" />
		</property>	
		<property name="name">
			<column name="NAME" />
		</property>	
		<property name="value">
			<column name="VALUE" />
		</property>	
		<property name="created_On" type="timestamp">
			<column name="CREATED_ON" />
		</property>
		<property name="changed_On" type="timestamp">
			<column name="CHANGED_ON" />
		</property>							
 	</class>
</hibernate-mapping>
