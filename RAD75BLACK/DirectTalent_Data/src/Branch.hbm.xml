<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

<class name="com.manpower.portal.mpnet.hbn.beans.Branch" table="V_BRANCHES">
	
		<!--  	
				Alexander Todorov 
				Date 07.02.2006
				The branches will be accessed using the view.
		 -->

		<id name="id" type="long" column="BRANCH_ID"/>
		
		<property name="siteId" 	column="SITE_ID"/>			
		<property name="language" 	column="LANG"/>			
		<property name="branchCode" column="CODE"/>
		<property name="street" column="STREET"/>
		<property name="address1" column="ADDRESS1"/>
		<property name="address2" column="ADDRESS2"/>		
		<property name="branchName"	column="BRANCHNAME"/>
		<property name="state"		column="STATE"/>
		<property name="location"	column="PREFERED_LOCATION"/>
		<property name="speciality" column="SPECIALITY"/>
		<property name="email" column="EMAIL"/>
		<property name="phoneNumber" column="PHONENUMBER"/>
		<property name="faxNumber" column="FAXNUMBER"/>
		<property name="postalCode" column="POSTALCODE"/>
		<property name="country" column="COUNTRY"/>
		<property name="city" column="CITY"/>
		<property name="locationInfo" column="LOCATION_INFO"/>
		<property name="operationHours" column="OPERATION_HOURS"/>
		<property name="comments" column="ADDITIONAL_INFO"/>	
		<property name="externalId" column="EXTERNAL_ID"></property>
		<property name="mapId" column="MAP_ID"/>
		<property name="mapUrl" column="MAP_URL"/>	
		<property name="latitude" column="LATITUDE"/>
		<property name="longitude" column="LONGITUDE"/>				
 	</class>
</hibernate-mapping>
