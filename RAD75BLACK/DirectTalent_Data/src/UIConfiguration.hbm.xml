<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

<class name="com.manpower.portal.mpnet.hbn.beans.UIConfiguration" table="V_CONFIGURATION">
		
		<id name="id" type="long" column="ID" >
		</id>
		
		<property name="siteCode">
			<column name="SITECODE" />
		</property>
		
		<property name="binding">
			<column name="CONDIDIONAL_BINDING" />
		</property>		

		<property name="view">
			<column name="VIEWABLE" />
		</property>		
		
 	</class>
</hibernate-mapping>