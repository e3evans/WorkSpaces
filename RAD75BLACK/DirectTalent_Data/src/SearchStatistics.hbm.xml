<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.manpower.portal.mpnet.hbn.beans.SearchStatistics" table="SEARCHSTATISTICS">
		
		<id name="id" type="long" column="ID" >
			<generator class="sequence"> 
				<param name="sequence">SEARCHSTATISTICS_SEQ</param>
			</generator>
		</id>
		
		<property name="candidateId">
			<column name="CANDIDATE_ID"/>
		</property>
		
		<property name="siteId">
			<column name="SITE_ID"/>
		</property>
		
		<property name="resultsFound">
			<column name="RESULTS_FOUND" />
		</property>		
		
		<property name="dateExecuted">
			<column name="DATE_EXECUTED" />
		</property>		

		<property name="timeConsumed">
			<column name="TIME_CONSUMED" />
		</property>		
		
 	</class>
</hibernate-mapping>
