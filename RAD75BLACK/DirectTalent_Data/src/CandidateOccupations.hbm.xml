<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

<class name="com.manpower.portal.mpnet.hbn.beans.CandidateOccupations" table="CANDIDATEOCCUPATIONS">
		<id name="id" type="long" column="ID" >
			<generator class="sequence"> 
				<param name="sequence">CANDIDATEOCCUPATIONS_SEQ</param>
			</generator>
		</id>

		<property name="candidateId" column="CANDIDATE_ID"></property>	
		<property name="occupationId" column="OCCUPATION_ID"></property>	
		
 	</class>
</hibernate-mapping>