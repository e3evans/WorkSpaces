<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.manpower.directalent.reporting.hbn.beans.OracleReportFileBean" table="DT_REPORTS" >
 		<id name="id"  column="id" />
		<property name="rpt_name" column="rpt_name" />
		<property name="report" column="report" type="text"/>
		<property name="mime_type" column="mime_type"/>
		<property name="sitename" column="sitename"/>
		<property name="rpt_descr" column="rpt_descr"/>
		<property name="created_on" column="created_on" type="timestamp"/>
	</class>
</hibernate-mapping>
