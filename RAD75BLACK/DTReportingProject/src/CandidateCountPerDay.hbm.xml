<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.manpower.directalent.reporting.hbn.beans.CandidateCountPerDayBean" table="DT_CANDIDATES" >
 		<id name="id"  column="sitename" />
		<property name="site_id" column="site_id" type="long"/>
		<property name="count" column="count" type="long"/>
		<property name="sitecode" column="sitecode"/>
		<property name="countdate" column="countdate" type="timestamp"/>
	</class>
</hibernate-mapping>
