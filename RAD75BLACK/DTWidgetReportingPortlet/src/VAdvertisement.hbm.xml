<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.manpower.directalent.rss.hbn.beans.VAdvertismentHibBean" table="V_ADVERTISEMENTS" >
 		<id name="id" type="long" column="advertisementid" />
		<property name="site_id" column="site_id" type="long"/>
		<property name="jobtitle" column="jobtitle"/>
		<property name="jobdescription" column="jobdescription"/>
		<property name="created_on" column="created_on" type="timestamp"/>
		<property name="changed_on" column="changed_on" type="timestamp"/>
		<property name="updatedon" column="updatedon" type="timestamp"/>
		<property name="expirationdate" column="expirationdate" type="timestamp"/>
	</class>
</hibernate-mapping>
