<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

<class name="com.manpower.directalent.rss.hbn.beans.Report" table="REPORTS">
		<id name="id" type="long" column="ID" >
			<generator class="sequence"> 
				<param name="sequence">REPORT_SEQ</param>
			</generator>
		</id>

		<property name="reportName" type="string">
			<column name="RPT_NAME" />
		</property>
		
		<property name="createdOn" type="timestamp">
			<column name="CREATED_ON" />
		</property> 
		
		<property name="reportDescription">
			<column name="RPT_DESCR" />
		</property>
		
		<property name="siteName">
			<column name="SITENAME" />
		</property>
		
		<property name="mimeType">
			<column name="MIME_TYPE" />
		</property>
		
		<property name="displayFlag">
			<column name="DISPLAY_FLAG" />
		</property>
		
		<property name="statId">
			<column name="STAT_ID" />
		</property>

 	</class>
</hibernate-mapping>

