<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.manpower.directtalentseo.hbn.beans.TAdContact" table="ADVERTISEMENTCONTACTS"  >
 		<id name="id" type="long" column="advertcontactid" />
		<property name="site_id" column="site_id" type="long"/>
		<property name="name" column="name"/>
		<property name="address1"  column="address1"/>
		<property name="address2" column="address2"/>
		<property name="pobox" column="pobox"/>
		<property name="city" column="city"/>
		<property name="state" column="state"/>
		<property name="postalcode" column="postalcode"/>
		<property name="phonenumber" column="phonenumber"/>
		<property name="recruiter_user_id" column="recruiter_user_id"/>
	</class>
</hibernate-mapping>
